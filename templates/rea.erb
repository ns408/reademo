###This file managed by Puppet, see reademo (RealEstate demo module - Lnunns)
server {
    listen 80;
    server_name <%= @server_binding %> www.<%= @server_binding %>;

    #hardening prevent server ID leakage
    server_tokens        off;
    # Tell Nginx and Passenger where your app's 'public' directory is
    root /var/www/rea/public;

    # Turn on Passenger
    passenger_enabled on;
    passenger_ruby <%= @ruby_home %>;
}
